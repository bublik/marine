%nav.navbar.navbar-default{style: 'margin-top: 20px;'}
  .container-fluid{class: menu_style}
    .collapse.navbar-collapse
      /left
      %ul.nav.navbar-nav
        - if user_signed_in?
          - if current_user.admin?
            %li= link_to('Agencies', agencies_path)
            %li= link_to('Users', users_path)

          - if current_user.crewing?
            %li= link_to('Managers', managers_path)
            %li= link_to('Users', users_path)

          - if current_user.manager?
            %li= link_to('Users', users_path)

        %li= link_to('Admin interface', root_url(subdomain: 'admin'))

      /right
      %ul.nav.navbar-nav.navbar-right
        - if user_signed_in?
          %li= link_to 'Edit account', edit_user_registration_path
          %li= link_to 'Logout', destroy_user_session_path, :method=>'delete'
        - else
          %li= link_to 'Login', new_user_session_path
          %li= link_to 'Sign up', new_user_registration_path
